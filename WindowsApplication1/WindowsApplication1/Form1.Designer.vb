    Private Sub btn_smdgn_Click(sender As Object, e As EventArgs) Handles btn_smdgn.Click
        Label3.Text = TextBox1.Text
        Dim sign As Char
        sign = Label2.Text
        Dim n1 As Double
        n1 = Convert.ToDouble(Label1.Text)
        Dim n2 As Double
        n2 = Convert.ToDouble(Label3.Text)
        Dim n3 As Double

        Select Case (sign)
            Case "+"
                n3 = n1 + n2
            Case "-"
                n3 = n1 - n2
            Case "*"
                n3 = n1 * n2
            Case "/"
                n3 = n1 / n2

        End Select
        TextBox1.Text = Convert.ToString(n3)
    End Sub
